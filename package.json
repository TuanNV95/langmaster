{
  "homepage": "https://tuannv95.github.io/langmaster/",
  "private": true,
  "name": "langmaster",
  "version": "7.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint .",
    "format": "prettier-eslint --write '**/*.js/*.html/*.png'",
    "minify:css": "uglifycss public/stylesheets/style.css --output public/stylesheets/style.css",
    "build": "npm-run-all -s build:prod minify:css",
    "build:prod": "cross-env MODE=production webpack",
    "build:dev": "cross-env MODE=development webpack",
    "watch": "cross-env MODE=development webpack --watch",
    "serve": "webpack-dev-server",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "keywords": [
    "yui540",
    "yui540.graphics",
    "css animation"
  ],
  "repository": "git@github.com:tuannv95/langmaster.git",
  "author": "TuanNV <nguyentuan060196@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-core": "6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eslint": "5.1.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "gh-pages": "^3.1.0",
    "gh-pages-deploy": "^0.5.1",
    "husky": "0.14.3",
    "import-glob-loader": "1.1.0",
    "lint-staged": "7.2.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "4.1.3",
    "prettier": "1.13.7",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "sass-loader": "7.0.3",
    "uglifycss": "0.0.29",
    "url-loader": "1.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "env-cmd": "10.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "^3.4.3",
    "redux": "4.0.0"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "cname": "nope.org",
    "prep": [
      "build-sass",
      "optimize-img"
    ],
    "commit": "a custom commit message",
    "post": [
      "clean-source"
    ],
    "noprompt": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
